<h2>
	<ul>
		<li>
			<a href="{{path('auto_subcategory')}}">Subcategory</a>
		</li>
		<li>
			<a href="{{path('auto_product')}}">Product</a>
		</li>
		<li>
			<a href="{{path('auto_slider')}}">Slider</a>
		</li>
	</ul>
</h2>
<h1>Category list</h1>

<table class="records_list">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td><a href="{{ path('auto_category_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{{ entity.name }}</td>
            <td>
                <ul>
                    <li>
                        <a href="{{ path('auto_category_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('auto_category_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>

<ul>
    <li>
        <a href="{{ path('auto_category_new') }}">
            Create a new entry
        </a>
    </li>
</ul>
