<head>
	<meta http-equiv="Content-type" content="text/html; charset=utf-8" />
	<title>Comertial</title>
        <script src="http://code.jquery.com/jquery-1.6.3.min.js" type="text/javascript" charset="utf-8"></script>
	<link rel="stylesheet" href="{{asset('bundles/jetshop/css/style.css')}}" type="text/css" media="all" />
        <link rel="shortcut icon" href="{{asset('bundle/jetshop/images/logo.ico')}}" type="image/x-icon"/>
	<!--[if let IE 6]>
		<style type="text/css" media="screen">
			.tabbed { height:420px; }
		</style>
	<![endif]-->
	<script src="{{asset('bundles/jetshop/js/jquery.jcarousel.pack.js')}}" type="text/javascript"></script>
	<script src="{{asset('bundles/jetshop/js/jquery.slide.js')}}" type="text/javascript"></script>
	<script src="{{asset('bundles/jetshop/js/jquery-func.js')}}" type="text/javascript"></script>
        
<script type="text/javascript">

  var _gaq = _gaq || [];
  _gaq.push(['_setAccount', 'UA-26775955-1']);
  _gaq.push(['_trackPageview']);

  (function() {
    var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
    ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
    var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
  })();

</script>
        
</head>
<body>
<!-- Top -->
<div id="top">
	
        <!-- Shell -->
	<div class="shell">
		
		<!-- Header -->
		<div id="header">
			<h1 id="logo"><a href="{{path('entrada_index', {'category': 'home'})}}">Comertial</a></h1>
			<div id="navigation">
				<ul>
				    <li><a href="{{path('entrada_index', {'category': 'home'})}}">Home</a></li>
                                    <li class="last"><a href="#">Contacto</a></li>
				</ul>
			</div>
		</div>
		<!-- End Header -->
		
		<!-- Slider -->
		<div id="slider">
			<div id="slider-holder">
				<ul>
                                    {% for aux in slider %}
                                        <li><a href="#"><img src="{{ asset(aux.getWebPath) }}" alt="" /></a></li>
				    {% endfor %}
				</ul>
			</div>
			<div id="slider-nav">
				<a href="#" class="prev">Previous</a>
				<a href="#" class="next">Next</a>
			</div>
		</div>
		<!-- End Slider -->
		
	</div>
</div>
<!-- Top -->

<!-- Main -->
<div id="main">
	<div class="shell">
		
		<!-- Search, etc -->
		<div class="options">
                    <ul id="category">
                        {% for aux in categories %}
                            {% if aux.name==category %}
                                <li><a class="active" href="{{ path('entrada_index', { 'category': (aux.name|lower) }) }}">{{aux.name|title}}</a></li>
                            {% else %}
                                <li><a href="{{ path('entrada_index', { 'category': (aux.name|lower) }) }}">{{aux.name|title}}</a></li>
                            {% endif %}    
                        {% endfor %}
                    </ul>
		</div>
		
		<!-- End Search, etc -->
		
		<!-- Content -->
		<div id="content">
			
			<!-- Tabs -->
			<div class="tabs">
				<ul>
                                    {% for aux in subcategories %}
                                        {% if loop.first %}
                                            <li><a href="#" class="active"><span>{{ aux.name }}</span></a></li>
                                        {% else %}    
                                            <li><a href="#"><span>{{ aux.name }}</span></a></li>
                                        {% endif %}
                                    {% endfor %}
				</ul>
			</div>
			<!-- Tabs -->
			
			<!-- Container -->
			<div id="container">
				
                                <!-- Tabbed -->
				<div class="tabbed">
					
					<!-- Tabs Content -->
                                        {% for aux in subcategories %}
                                            {% if loop.first %}
                                                <div class="tab-content" style="display:block;">
                                            {% else %}
                                                <div class="tab-content">
                                            {% endif %}
                                                    <div class="items">
                                                            <div class="cl">&nbsp;</div>
                                                                <ul class="gallery clearfix">
                                                                    {% for auxproduct in product %}
                                                                        {% if auxproduct.subcategory == aux.name %}
                                                                            <li>
                                                                                <div class="image">
                                                                                    <a href="{{asset(auxproduct.getWebPath)}}" rel="prettyPhoto[{{aux.name}}]"><img src="{{asset(auxproduct.getWebPath)}}" height="83" width="126" alt="" /></a>
                                                                                </div>
                                                                                <p>
                                                                                        Num. Referencia: <span>{{auxproduct.getItemNumber}}</span><br />
                                                                                        Lista Tallas : <span>{{auxproduct.getSizeList}}</span><br />
                                                                                        Nombre Producto: <a href="#">{{auxproduct.getName}}</a>
                                                                                </p>
                                                                                <p class="price">Precio: <strong>{{auxproduct.getPrice}} â‚¬</strong></p>
                                                                            </li>
                                                                        {% endif %}
                                                                    {% endfor %}
                                                                </ul>
                                                            <div class="cl">&nbsp;</div>
                                                    </div>
                                                </div>
                                        {% endfor %}
					<!-- End Tabs Content -->
				</div>
                                <!-- End Tabbed -->
				<div style="width: 728px; margin: 0 auto">
